package com.text.exam02;
public class JavaStructure {
	public static int sum(int a, int b) {
		return a+b;
	}
	public static void main(String[]argv) {
		int i = 10;
		long l value = 10L;
		double d value = 10;
		float f value = 10;
		int j;
		char a;
		
		i = sum(i, 10);
		a = '가';
		System.out.println("Hello, World.");
		System.out.println("Integer타입i="+integer.to.String(i));
		System.out.println("double타입d value="+Double.toString(d value));
		System.out.println("float타입f value="+Float.toString(f value));
		System.out.println("long타입l value="+Long.toString(l value));
		System.out.println("char타입a="+a);
	}
}
